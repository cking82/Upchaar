<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_frame_patient"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="in.project.com.upchaar.PatientDash"
    tools:showIn="@layout/app_bar_patient_dash"
    android:background="@color/black">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name :"
            android:layout_marginTop="200dp"
            android:textSize="25dp"
            android:textColor="#00ccaa"/>

        <TextView
            android:id="@+id/name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Raghav"
            android:layout_marginTop="200dp"
            android:layout_marginLeft="100dp"
            android:textSize="25dp"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email Id :"
            android:layout_marginTop="250dp"
            android:textSize="25dp"
            android:textColor="#00ccaa"/>

        <TextView
            android:id="@+id/email1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="raghavkapoor16@hotmail.com"
            android:layout_marginTop="255dp"
            android:layout_marginLeft="120dp"
            android:textSize="19dp"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Address : "
            android:layout_marginTop="300dp"
            android:textSize="25dp"
            android:textColor="#00ccaa"/>

        <TextView
            android:id="@+id/add1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="G-171 Preet Vihar, Delhi"
            android:layout_marginTop="300dp"
            android:layout_marginLeft="120dp"
            android:textSize="25dp"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/gender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gender : "
            android:layout_marginTop="350dp"
            android:textSize="25dp"
            android:textColor="#00ccaa"/>

        <TextView
            android:id="@+id/gender1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Male"
            android:layout_marginTop="350dp"
            android:layout_marginLeft="100dp"
            android:textSize="25dp"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/dob"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Date of Birth : "
            android:layout_marginTop="400dp"
            android:textSize="25dp"
            android:textColor="#00ccaa"/>

        <TextView
            android:id="@+id/dob1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="16/06/1997"
            android:layout_marginTop="400dp"
            android:layout_marginLeft="160dp"
            android:textSize="25dp"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contact Number : "
            android:layout_marginTop="450dp"
            android:textSize="25dp"
            android:textColor="#00ccaa"/>

        <TextView
            android:id="@+id/no1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9910033452"
            android:layout_marginTop="450dp"
            android:layout_marginLeft="200dp"
            android:textSize="25dp"
            android:textColor="#ffffff"/>

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/fab_margin"
            app:srcCompat="@drawable/update" />

</RelativeLayout>
